{
  "name": "ag-ui-react",
  "private": true,
  "author": "Matyas Furtos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electronic33/ag-ui-react.git"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "tw-plugins/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && manypkg check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "**/*.{md,yml,json,eslintrc,prettierrc}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "prestart": "yarn && yarn bootstrap && yarn dedup",
    "check:pkgs": "manypkg check",
    "fix:pkgs": "manypkg fix",
    "gen:pkg": "ts-node scripts/plopfile.ts",
    "verify": "lint-staged && manypkg check",
    "storybook": "start-storybook -p 6006",
    "storybook:ci": "start-storybook --quiet --ci -p 6006",
    "build-storybook": "build-storybook",
    "test": "start-server-and-test 'yarn storybook:ci' http://localhost:6006 'yarn test:lerna'",
    "test:lerna": "lerna run test",
    "cy:open": "cypress open",
    "commit": "git-cz",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "postinstall": "yarn check:pkgs",
    "version:pkgs": "changeset version",
    "release": "changeset publish",
    "changeset": "changeset",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@changesets/action": "changesets/action",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/config-lerna-scopes": "^12.1.4",
    "@commitlint/prompt": "^12.1.4",
    "@commitlint/prompt-cli": "^12.1.4",
    "@headlessui/react": "^1.3.0",
    "@manypkg/cli": "^0.18.0",
    "@popperjs/core": "^2.9.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-a11y": "^6.3.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.3.0",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-links": "^6.3.0",
