    "@storybook/addon-storyshots": "^6.3.0",
    "@storybook/addons": "^6.3.0",
    "@storybook/react": "next",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/classnames": "^2.3.1",
