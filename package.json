{
  "name": "ag-ui-react",
  "private": true,
  "version": "1.0.4",
  "author": "Matyas Furtos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electronic33/ag-ui-react.git"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "tw-plugins/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint", "eslint --fix"
    ],
    "**/*.{md,yml,json,babelrc,eslintrc,prettierrc}": [
      "eslint", "eslint --fix"
    ]
  },
  "scripts": {
    "pkg": "manypkg run",
    "prestart": "yarn && yarn bootstrap && yarn dedup",
    "check:pkgs": "manypkg check",
    "fix:pkgs": "manypkg fix",
    "version:pkgs": "changeset version",
    "gen:pkg": "ts-node scripts/plopfile.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "cy:open": "cypress open",
    "commit": "npx git-cz",
    "build": "lerna run build --stream",
    "build:types": "lerna run build:types --stream",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "postinstall": "yarn check:pkgs",
    "release": "changeset publish",
    "version:lerna": "lerna version",
    "publish:lerna": "lerna publish"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@changesets/changelog-github": "^0.3.0",
    "@changesets/cli": "^2.14.1",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/config-lerna-scopes": "^12.0.1",
    "@headlessui/react": "^0.3.1",
    "@manypkg/cli": "^0.17.0",
    "@popperjs/core": "^2.9.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "next",
    "@tailwindcss/postcss7-compat": "npm:@tailwindcss/postcss7-compat",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "add": "^2.0.6",
    "autoprefixer": "^10.0.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "commitizen": "^4.2.3",
    "css-loader": "^5.1.3",
    "cypress": "^6.8.0",
    "date-fns": "^2.19.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "12.3.1",
